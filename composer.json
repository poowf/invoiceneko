{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "arcanedev/no-captcha": "^7.0",
        "barryvdh/laravel-snappy": "^0.4.1",
        "binarytorch/larecipe": "^1.2",
        "doctrine/dbal": "^2.8",
        "erusev/parsedown": "^1.7",
        "fideloper/proxy": "^4.0",
        "geoip2/geoip2": "^2.9",
        "guzzlehttp/guzzle": "^6.3",
        "iatstuti/laravel-cascade-soft-deletes": "^1.3",
        "intervention/image": "^2.4",
        "jenssegers/agent": "^2.6",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.7.*",
        "laravel/horizon": "^2.0",
        "laravel/telescope": "^1.0",
        "laravel/tinker": "^1.0",
        "laravelista/ekko": "^1.5",
        "maatwebsite/excel": "~3.1.0",
        "owen-it/laravel-auditing": "^8.0",
        "poowf/otter": "dev-master",
        "pragmarx/google2fa-laravel": "^0.2.0",
        "pragmarx/recovery": "^0.1.0",
        "predis/predis": "^1.1",
        "rinvex/countries": "^5.0",
        "rollbar/rollbar-laravel": "^4.0",
        "sentry/sentry-laravel": "^0.11.0",
        "silber/bouncer": "dev-master",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "simshaun/recurr": "^3.0",
        "torann/geoip": "^1.0",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.5",
        "beyondcode/dusk-dashboard": "^1.0",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^4.0",
        "lubusin/laravel-decomposer": "^1.2",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models -N"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
